{
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "comma-dangle": 1,
    "valid-jsdoc": 0, 
    "block-scoped-var": 1,
    "curly": [0, "multi"],
    "dot-location": [0, "object"],
    "eqeqeq": [2, "allow-null"],
    "no-loop-func": 0,
    "no-multi-spaces": 0,
    "no-self-compare": 1,
    "strict": 0,
    "no-shadow": 0,
    "no-undefined": 0,
    "no-unused-vars": [2, {"vars": "all", "args": "none"}],
    "no-use-before-define": [0, "nofunc"],
    "no-new": 0,
    "brace-style": [1, "stroustrup"],
    "camelcase": 0,
    "comma-spacing": [0, {"before": false, "after": true}], 
    "consistent-this": [0, "_this"],
    "eol-last": 1,
    "key-spacing": 0,
    "new-cap": 0,
    "no-trailing-spaces": 0,
    "no-mixed-spaces-and-tabs": 0,
    "no-underscore-dangle": 0,
    "quotes": [0, "single"],
    "semi-spacing": 0,
    "space-infix-ops": 0,
    "space-unary-ops": 0
  }
}
